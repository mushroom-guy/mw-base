"UnlitGeneric"
{
	"$basetexture"	"mw19_thermalnoise.vmt"
	"$translucent" 	"1"
	"$vertexalpha" "1"
	"$vertexcolor" "1"
	
	"$time" "0"
	
	Proxies
	{
		CurrentTime
		{
			resultVar $time
		}
		TextureScroll
		{
			texturescrollvar $basetexturetransform
			texturescrollrate 100
			texturescrollangle $time
		}
	}
}